  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run without source dir",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/ytdlp-to-show/main.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/.venv/bin/python",
      "args": ["--url-file", "urls.json", "--download-dir", "ytdlp-output", "--library-dir", "library"],
      "env": {
      }
    },
    {
      "name": "Run with source dir",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/ytdlp-to-show/main.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/.venv/bin/python",
      "args": ["--url-file", "urls.json", "--download-dir", "ytdlp-output", "--library-dir", "library","--source-dir","source-dir"],
      "env": {
      }
    }
  ]
}