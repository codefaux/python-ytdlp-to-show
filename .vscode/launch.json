{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run without source dir",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/ytdlp-to-show/main.py",
      "console": "integratedTerminal",
      "autoReload": {
        "enable": true
      },
      "python": "${workspaceFolder}/.venv/bin/python",
      "args": ["--url-file", "urls.json", "--download-dir", "ytdlp-output", "--channel-library-dir", "library-channels", "--playlist-library-dir", "library-playlists"],
      "env": {
        "MATCHER_THREADS": "12",
      }
    },
    {
      "name": "Run with source dir",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/ytdlp-to-show/main.py",
      "console": "integratedTerminal",
      "autoReload": {
        "enable": true
      },
      "python": "${workspaceFolder}/.venv/bin/python",
      "args": ["--url-file", "urls.json", "--download-dir", "ytdlp-output", "--channel-library-dir", "library-channels", "--playlist-library-dir", "library-playlists"],
      "env": {
        "MATCHER_THREADS": "12",
      }
    }
  ]
}